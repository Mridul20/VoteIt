<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.onlinevotingsystem">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/loo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/loo"
        android:supportsRtl="true"
        android:theme="@style/Theme.OnlineVotingSystem">
        <activity android:name=".activities.OfficerActivity"/>
        <activity android:name=".activities.AdminActivity"/>
        <activity android:name=".activities.UserActivity" />
        <activity android:name=".activities.StartupActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>